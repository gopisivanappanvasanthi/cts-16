<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<pipelines>
			<httpRequestBegin>
				<processor type="Cts.Feature.Common.Pipelines.PageNotFoundResolver, Cts.Feature.Common"
						   patch:after="processor[@type='Sitecore.Pipelines.HttpRequest.ItemResolver, Sitecore.Kernel']">
				</processor>
			</httpRequestBegin>
			<httpRequestProcessed>
				<processor type="Cts.Feature.Common.Pipelines.HandleServerError, Cts.Feature.Common">
					
				</processor>
			</httpRequestProcessed>
		</pipelines>
	</sitecore>
</configuration>
